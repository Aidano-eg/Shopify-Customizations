<script>
  document.addEventListener("DOMContentLoaded", function () {
    const randomInterestedCount = Math.floor(Math.random() * 100) + 1;
    const interestedText = document.querySelector(".custom-interested-text");
    if (interestedText) {
      interestedText.textContent = `${randomInterestedCount} people are interested in this product`;
    }
  });
</script>





























