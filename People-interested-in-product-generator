<script>
  document.addEventListener("DOMContentLoaded", function() {
    // Generate a random number between 1 and 100 (you can adjust the range as needed)
    const randomInterestedCount = Math.floor(Math.random() * 100) + 1;

    // Update the text content dynamically
    const interestedCountElement = document.getElementById("interestedCount");
    interestedCountElement.textContent = `${randomInterestedCount} people are interested in this kitten`;
  });
</script> 



